<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name="com.blinkreceipt.ocr.BlinkReceiptDemoApplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Blinkreceiptdemo">
        <activity
            android:name="com.blinkreceipt.ocr.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Blinkreceiptdemo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.microblink.ProductIntelligence"
            android:value="" />

        <meta-data
            android:name="com.microblink.LicenseKey"
            android:value="" />

    </application>

</manifest>